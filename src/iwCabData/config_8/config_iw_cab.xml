        <key>3</key>
    </keys>
    <keys number="3">
        <key>InternalMass</key>
        <key>central_intMass</key>
        <key>3</key>
    </keys>
    <range>
        <max>100.0</max>
        <min>30.0</min>
    </range>
    <description>
        The internal mass area. 
   </description>
</calibration_parameter>


