<?xml version="1.0" encoding="UTF-8"?>
<runSimulatorConfig>

<calibration_parameter>
    <name>pitchedRoofInsuThickness</name>
    <keys number="3">
        <key>Material</key>
        <key>rip40_batt_insulation_glass_fiber_30cm</key>
        <key>2</key>
    </keys>
    <range>
        <max>0.3</max>
        <min>0.05</min>
    </range>
    <description>
        The piched roof insulation thickness.
   </description>
</calibration_parameter>

<calibration_parameter>
    <name>mullSurfaceMetalThickness</name>
    <keys number="3">
        <key>Material</key>
        <key>copper_2mm</key>
        <key>2</key>
    </keys>
    <range>
        <max>0.01</max>
        <min>0.001</min>
    </range>
    <description>
        The mullion surface metal thickness. 
   </description>
</calibration_parameter>

<calibration_parameter>
    <name>extWinU</name>
    <keys number="3">
        <key>WindowMaterial:SimpleGlazingSystem</key>
        <key>iw_ext_win_simple</key>
        <key>1</key>
    </keys>
    <range>
        <max>3.0</max>
        <min>1.5</min>
    </range>
    <description>
        The external window U value. 
   </description>
</calibration_parameter>

<calibration_parameter>
    <name>extWinSHGC</name>
    <keys number="3">
        <key>WindowMaterial:SimpleGlazingSystem</key>
        <key>iw_ext_win_simple</key>
        <key>4</key>
    </keys>
    <range>
        <max>0.65</max>
        <min>0.25</min>
    </range>
    <description>
        The external window SHGC value. 
   </description>
</calibration_parameter>

<calibration_parameter>
    <name>intMass</name>
    <keys number="3">
        <key>InternalMass</key>
